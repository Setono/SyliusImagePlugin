{
    "name": "setono/sylius-image-plugin",
    "description": "Setono example plugin for Sylius.",
    "license": "MIT",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/persistence": "^2.3",
        "knplabs/gaufrette": "^0.8",
        "liip/imagine-bundle": "^2.6",
        "psr/event-dispatcher": "^1.0",
        "setono/doctrine-object-manager-trait": "^1.0",
        "spatie/data-transfer-object": "^2.8",
        "sylius/resource-bundle": "^1.8",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.5",
        "symfony/filesystem": "^4.4 || ^5.0",
        "symfony/http-client": "^4.4 || ^5.0",
        "symfony/http-client-contracts": "^1.1 || ^2.5",
        "symfony/http-foundation": "^4.4 || ^5.0.7",
        "symfony/messenger": "^4.4 || ^5.0",
        "symfony/mime": "^4.4 || ^5.0",
        "symfony/workflow": "^4.4 || ^5.0",
        "twig/twig": "^2.14",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "api-platform/core": "^2.6",
        "friendsofsymfony/oauth-server-bundle": ">2.0.0-alpha.0 ^2.0@dev",
        "lexik/jwt-authentication-bundle": "^2.14",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.1",
        "roave/security-advisories": "dev-latest",
        "setono/code-quality-pack": "^2.1.3",
        "sylius/admin-api-bundle": "^1.10",
        "sylius/sylius": "~1.10.6",
        "symfony/debug-bundle": "^4.4 || ^5.3",
        "symfony/dotenv": "^4.4 || ^5.3",
        "symfony/intl": "^4.4 || ^5.3",
        "symfony/maker-bundle": "^1.36",
        "symfony/property-info": "^4.4 || ^5.3",
        "symfony/serializer": "^4.4 || ^5.3",
        "symfony/web-profiler-bundle": "^4.4 || ^5.3",
        "symfony/webpack-encore-bundle": "^1.13"
    },
    "conflict": {
        "symfony/security-bundle": "5.4.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\SyliusImagePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\SyliusImagePlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "unused": [
            "symfony/http-client"
        ]
    },
    "scripts": {
        "analyse": [
            "@ensure-test-container-exists",
            "psalm"
        ],
        "check-style": "ecs check",
        "ensure-test-container-exists": "[[ -f tests/Application/var/cache/test/Tests_Setono_SyliusImagePlugin_Application_KernelTestDebugContainer.xml ]] || tests/Application/bin/console cache:warmup --env=test",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
